    "reflect-metadata": "0.2.2",
    "starknet": "6.7.0",
    "tslib": "2.6.3",
    "type-graphql": "2.0.0-rc.2",
    "typegraphql-prisma": "0.28.0",
    "yahoo-finance2": "2.11.3"
  },
  "devDependencies": {
    "@eslint/js": "9.0.0",
    "@types/node": "^24.3.0",
    "@types/pg": "^8.15.5",
    "browserify": "17.0.0",
    "drizzle-kit": "^0.30.3",
    "drizzle-prisma-generator": "^0.1.7",
    "eslint": "9.0.0",
    "eslint-config-airbnb": "19.0.4",
