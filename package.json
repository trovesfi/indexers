{
  "name": "indexers",
  "version": "1.0.0",
  "description": "apibara indexers to index DeFi Spring $STRK claim data",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "start-subscriptions": "apibara run --allow-env=.env src/strkfarm/loanguard/subscriptions.ts --sink-id=9147567454926435867502085893747",
    "start-rebalances": "apibara run --allow-env=.env src/strkfarm/loanguard/rebalances.ts --sink-id=540219989265038396581235",
    "start-liquidations": "apibara run --allow-env=.env src/zklend/liquidations.ts --sink-id=100",
    "start-liquidations2": "apibara run --allow-env=.env src/zklend/liquidations.ts --sink-id=105",
    "start-dnmm-actions": "apibara run --allow-env=.env src/strkfarm/dnmm/user_actions.ts --sink-id=120",
    "start-dep-withdraw": "apibara run --allow-env=.env src/strkfarm/deposits-withdraws.ts --sink-id=130 --status-server-address=0.0.0.0:4130",
    "start-harvests": "apibara run --allow-env=.env src/strkfarm/harvests.ts --sink-id=160 --status-server-address=0.0.0.0:4140",
    "postinstall": "npx prisma generate",
    "dev": "apibara dev",
    "apibara:build": "apibara build",
    "apibara:start": "apibara start",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push"
  },
  "author": "akiraonstarknet",
  "license": "ISC",
  "dependencies": {
    "@apibara/indexer": "^2.1.0-beta.37",
    "@apibara/plugin-drizzle": "2.0.0-beta.37",
    "@apibara/protocol": "^2.1.0-beta.37",
    "@apibara/starknet": "^2.1.0-beta.37",
    "@apollo/server": "4.11.0",
    "@hemantwasthere/monitoring-sdk": "^3.2.9",
    "@prisma/client": "5.18.0",
    "@strkfarm/sdk": "1.1.4",
    "apibara": "^2.1.0-beta.37",
    "dotenv": "16.4.5",
    "drizzle-orm": "^0.39.0",
    "express": "4.19.2",
    "graphql": "16.9.0",
    "graphql-fields": "2.0.3",
